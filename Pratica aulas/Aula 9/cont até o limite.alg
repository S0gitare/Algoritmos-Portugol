algoritmo "Conte de 0 até onde o usuário quiser"

var

    contador:Inteiro
    L: Inteiro

inicio

    escreval("Até onde você deseja contar? ")
    leia(L)
    contador <- 0
    enquanto (contador <= L) faca
        escreval(contador)
        contador <- contador +1
    fimenquanto

    escreval("Terminou a contagem")

fimalgoritmo